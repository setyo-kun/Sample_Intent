<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.wecantyo.app.sample_intent.MainActivity">


    <Button
        android:text="Ke Activity Sub 1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button_activity_sub_1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text=""
        android:ems="10"
        android:id="@+id/text_intent"
        android:layout_below="@+id/button_activity_sub_1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Ke Activity Sub 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button_activity_sub_2"
        android:layout_below="@+id/text_intent"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Ke Activity Dial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button_activity_dial"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/text_intent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
